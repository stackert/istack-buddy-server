{
  "name": "istack-buddy-server-take1",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:cov": "jest --coverage --maxWorkers=50%",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --maxWorkers=25%",
    "db:create": "ts-node docs-living/database/scripts/create-database.ts",
    "db:seed": "ts-node docs-living/database/scripts/seed-database.ts",
    "robot:anth:test": "ts-node docs-living/scripts/test-anthropic-robot.ts",
    "re:import": "cp ../istack-buddy-utilities/istack-buddy-utilities-1.0.0.tgz .  && npm uninstall istack-buddy-utilities && npm install istack-buddy-utilities-1.0.0.tgz",
    "test:feedback-tools": "ts-node docs-living/scripts/test-feedback-tools.ts",
    "test:direct-commands": "ts-node docs-living/scripts/test-direct-feedback-commands.ts",
    "test:logging": "ts-node scripts/test-logging.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@aws-sdk/client-bedrock": "^3.848.0",
    "@aws-sdk/client-bedrock-runtime": "^3.848.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.19",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.19",
    "@slack/bolt": "^3.21.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "istack-buddy-utilities": "file:istack-buddy-utilities-1.0.0.tgz",
    "openai": "^5.6.0",
    "pg": "^8.11.3",
    "predicate-tree-advanced-poc": "file:predicate-tree-advanced-poc-2.0.2-beta.0.tgz",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.7",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.5.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "3.6.2",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/dev-debug",
      "dist",
      "coverage",
      ".spec.ts",
      ".d.ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": {
            "module": "commonjs",
            "target": "es2020",
            "skipLibCheck": true,
            "experimentalDecorators": true,
            "emitDecoratorMetadata": true
          }
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../jest.setup.js"
    ],
    "testTimeout": 10000,
    "maxWorkers": "50%",
    "cache": true,
    "cacheDirectory": "<rootDir>/../node_modules/.cache/jest"
  }
}
